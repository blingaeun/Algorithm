   ],
   "source": [
    "T = int(input())\n",
    "\n",
    "for _ in range(T):\n",
    "    m, n = map(int, input().split())\n",
    "    answer = 1\n",
    "    k = n - m\n",
    "    \n",
    "    while n > k:\n",
    "        answer *= n\n",
    "        n -= 1\n",
    "    while m > 1:\n",
    "        answer = answer // m\n",
    "        m -= 1\n",
    "    \n",
    "    print(answer)"
   ]
  }
